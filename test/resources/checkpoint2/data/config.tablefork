merge
    + load "checkpoint2/data/vitis-report.xml"
        + select
            + fields "FOLDER"
            + from "profile.AreaEstimates"
                + fields "Resources"
            * from
        * select
    + load "checkpoint2/data/decision_tree.yaml"
        + select
            + type "TABLE"
            + fields "params"
        * select
    + load "checkpoint2/data/profiling.json"
        + select
            + fields "functions"
        * select
        + unstack
        + select
            + fields "name" "time%"
        * select
        + max "time%"
+ rename "FOLDER" to "Folder"
+ save "Assignment 2.csv"